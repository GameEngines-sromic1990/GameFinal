                <Image type="Background" imageset="GlossySerpent" image="ProgressBar_HD_M" />
                <Image type="BottomEdge" imageset="GlossySerpent" image="ProgressBar_HD_B" />
                <Image type="TopRightCorner" imageset="GlossySerpent" image="ProgressBar_HD_TR_Redux" />
                <Image type="RightEdge" imageset="GlossySerpent" image="ProgressBar_HD_R_Redux" />
                <Image type="BottomRightCorner" imageset="GlossySerpent" image="ProgressBar_HD_BR_Redux" />
            </FrameComponent>
        </ImagerySection>

        <ImagerySection name="background_highlight">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="4" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="4" /></Dim>
                    <Dim type="Width" >
                        <UnifiedDim scale="1" type="Width">
                            <DimOperator op="Subtract">
                                <AbsoluteDim value="8" />
                            </DimOperator>
                        </UnifiedDim>
