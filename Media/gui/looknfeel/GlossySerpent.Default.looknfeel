                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>

        <ImagerySection name="background_highlight_redux">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="1" /></Dim> <!-- 4 - 3 -->
                    <Dim type="TopEdge" ><AbsoluteDim value="4" /></Dim>
