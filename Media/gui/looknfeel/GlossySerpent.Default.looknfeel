                <HorzFormat type="Stretched" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="label_bg">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge" >
                        <AbsoluteDim value="5">
                            <DimOperator op="Add">
                                <PropertyDim name="CaptionOffset" />
                            </DimOperator>
                        </AbsoluteDim>
                    </Dim>
                    <Dim type="TopEdge" >
                        <AbsoluteDim value="0" />
                    </Dim>
                    <Dim type="Width" >
                        <FontDim type="HorzExtent" padding="5">
                            <DimOperator op="Add">
                                <AbsoluteDim value="8" />
                            </DimOperator>
