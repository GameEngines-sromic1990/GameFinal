                <Image type="TopLeftCorner" imageset="GlossySerpent" image="ProgressBar_HD_TL" />
                <Image type="LeftEdge" imageset="GlossySerpent" image="ProgressBar_HD_L" />
                <Image type="BottomLeftCorner" imageset="GlossySerpent" image="ProgressBar_HD_BL" />
                <Image type="TopEdge" imageset="GlossySerpent" image="ProgressBar_HD_T" />
                <Image type="Background" imageset="GlossySerpent" image="ProgressBar_HD_M" />
                <Image type="BottomEdge" imageset="GlossySerpent" image="ProgressBar_HD_B" />
                <Image type="TopRightCorner" imageset="GlossySerpent" image="ProgressBar_HD_TR" />
                <Image type="RightEdge" imageset="GlossySerpent" image="ProgressBar_HD_R" />
                <Image type="BottomRightCorner" imageset="GlossySerpent" image="ProgressBar_HD_BR" />
            </FrameComponent>
        </ImagerySection>
        
        <ImagerySection name="border_section_redux">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" offset="0" /></Dim>
