    <!--
    ***************************************************
        GlossySerpent/TabControl
        (based on the Taharez Look)
    ***************************************************
    -->
    <WidgetLook name="GlossySerpent/TabControl">
        <Property name="TabButtonType" value="GlossySerpent/TabButton" />
        <Property name="TabTextPadding" value="{0,6}" />
        <PropertyDefinition name="TCScrollLRatio" initialValue="0.2" redrawOnWrite="true" /> <!-- The ratio is height / width -->
<!--        <PropertyDefinition name="TCScrollLRatio" initialValue="0.1" redrawOnWrite="true" layoutOnWrite="true" />-->
        <Child type="GlossySerpent/TabContentPane" nameSuffix="__auto_TabPane__">
            <Area>
                <Dim type="LeftEdge"><AbsoluteDim value="0" /></Dim>
                <Dim type="RightEdge"><UnifiedDim scale="1" type="Width" /></Dim>
                <Dim type="TopEdge">
                    <PropertyDim widget="__auto_TabPane__" name="EnableBottom">
                        <DimOperator op="Multiply">
                            <PropertyDim name="TabHeight" type="Height" />
                        </DimOperator>
                    </PropertyDim>
                </Dim>
                <Dim type="Height">
                    <UnifiedDim scale="1" type="Height">
                        <DimOperator op="Subtract">
                            <PropertyDim name="TabHeight" type="Height" />
                        </DimOperator>
                    </UnifiedDim>
                </Dim>
            </Area>
        </Child>
        <Child  type="GlossySerpent/TabButtonPane" nameSuffix="__auto_TabPane__Buttons">
            <Area>
                <Dim type="LeftEdge">
                    <PropertyDim name="TabHeight" type="Height" >
                        <DimOperator op="Multiply">
                            <PropertyDim name="TCScrollLRatio"/>
                        </DimOperator>
                    </PropertyDim>
                </Dim>
                <Dim type="RightEdge">
                    <UnifiedDim scale="1" type="Width" >
                        <DimOperator op="Subtract">
                            <PropertyDim name="TabHeight" type="Height" >
                                <DimOperator op="Multiply">
                                    <PropertyDim name="TCScrollLRatio"/>
                                </DimOperator>
                            </PropertyDim>
                        </DimOperator>
                    </UnifiedDim>
                </Dim>
                <Dim type="TopEdge">
                    <AbsoluteDim value="1">
                        <DimOperator op="Add">
